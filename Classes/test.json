\t\"l2\": [\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\t\"title\": \"Type\",\r\n\t\t\t\t\t\t\"field\": \"type\",\r\n\t\t\t\t\t\t\"type\": \"ddl\",\r\n\t\t\t\t\t\t\"values\": \"Warf or Jetty, Boat ramp, Other\"\r\n\t\t\t\t\t}\r\n\t\t\t\t]\r\n\t\t\t},\r\n\t\t\t{\r\n\t\t\t\t\"title\": \"Heritage\",\r\n\t\t\t\t\"field\": \"heritage\",\r\n\t\t\t\t\"l2\": [\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\t\"title\": \"Type\",\r\n\t\t\t\t\t\t\"field\": \"type\",\r\n\t\t\t\t\t\t\"type\": \"ddl\",\r\n\t\t\t\t\t\t\"values\": \"Aboriginal, Historic\"\r\n\t\t\t\t\t},\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\t\"title\": \"Issue\",\r\n\t\t\t\t\t\t\"field\": \"issue\",\r\n\t\t\t\t\t\t\"type\": \"ddl\",\r\n\t\t\t\t\t\t\"values\": \"Damage, Discovery, Removal, Trespass\"\r\n\t\t\t\t\t}\r\n\t\t\t\t]\r\n\t\t\t},\r\n\t\t\t{\r\n\t\t\t\t\"title\": \"Other\",\r\n\t\t\t\t\"field\": \"other\",\r\n\t\t\t\t\"l2\": []\r\n\t\t\t}\r\n\t\t];\r\n\r\n    if( this.inspection.data.issueClass ) {\r\n      this.setActiveIssue();\r\n    }\r\n  }\r\n\r\n  setActiveIssue() {\r\n    var that = this;\r\n    this.activeIssue = this.tsData.find( function(v){ return v.field === that.inspection.data.category;});\r\n    \r\n    try{\r\n        this._cdRef.detectChanges();\r\n    }\r\n    catch(ex){}\r\n  }\r\n\r\n    getDdlValues( value ){\r\n\t\treturn value.split(/\\s?,\\s?/);\r\n\t}\r\n\r\n  getBioSubDdlValues( key ) {\r\n    \r\n    if( !key ) return [];\r\n\r\n    try{\r\n    return this.getDdlValues( this.tsData.find( function(v){ return v.field == \"biosecurity\"; } )\r\n      .l2.find( function(v){ return v.field == \"env_bs_type\"; })\r\n      .l3.find( function(v){ return v.key == key; }).values);\r\n    }\r\n    catch(ex){\r\n      return [];\r\n    }\r\n  }\r\n\r\n  onValidate( inspection ){return true;}\r\n\r\n  onSave( inspection ){return true;}\r\n\r\n}",
